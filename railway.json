{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "dotnet build CultBot/CultBot.csproj -c Release"
  },
  "deploy": {
    "startCommand": "dotnet run --project CultBot/CultBot.csproj -c Release --no-build",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
